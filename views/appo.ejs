                    </li>
                    <li class="sidebar-dropdown">
                        <a href="/users/appo">
                            <i class="fas fa-calendar-check"></i>
                            <span>Schedule Appointment</span>
                        </a>
                    </li>
                    <li class="sidebar-dropdown">
                        <a href="/users/search">
                            <i class="fas fa-search-dollar"></i>
                            <span>Search Job</span>
                        </a>
                    </li>
                </ul>
            </div>
            <!-- sidebar-menu  -->
        </div>
        <!-- sidebar-content  -->
        <div class="sidebar-footer">
            <a href="/users/logout">
                <i class="fas fa-sign-out-alt"> LogOut</i>
            </a>
        </div>
    </nav>
    <!-- sidebar-wrapper  -->
    <main class="page-content">
        <div class="container-fluid">
            <h2>Stud2Emp - Schedule Appointment</h2>
            <hr>
            <div class="row">
                <script type="text/javascript" src="https://form.jotform.com/jsform/202466668418464"></script>

            </div>
            <hr>
        </div>
    </main>
