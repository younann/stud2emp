    <main class="page-content">
        <div class="container-fluid">
            <h2>Stud2Emp - Search Job</h2>
            <hr>
            <div class="row">
                <div id="form">
                    <div class="md-form mt-0">
                        <form class="form-inline active-cyan-4">
                            <input class="form-control form-control-sm mr-3 w-75" type="text" placeholder="Job Title"
                                id="searchj" aria-label="Search">
                            <i class="fas fa-search" aria-hidden="true"></i>
                            <input class="form-control form-control-sm mr-3 w-75" type="text" placeholder="Location" id="location"
                                aria-label="Search">    
                        </form>
                        <button class="btn btn-outline-success btn-rounded btn-sm my-0" onclick="searchit()">Search</button>
                    </div>
                </div>
                <div id="reslut">
                    
                </div>
            </div>

            <hr>
        </div>
    </main>
